    "version": "0.2.0",
    "configurations": [
        {
            "name": "queue",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--dart-define-from-file",
                "lib/secret/api-keys.json",
                "-d", "chrome","--web-port", "8000"
              ]
        },
        {
            "name": "queue(debug mobile)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--dart-define-from-file",
                "lib/secret/api-keys.json",
              ]
        },
        {
            "name": "queue (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define-from-file",
                "lib/secret/api-keys.json",
                "-d", "chrome","--web-port", "8000"
              ]
        },
        {
            "name": "queue (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define-from-file",
                "lib/secret/api-keys.json",
                "-d", "chrome","--web-port", "8000"
              ]
        }
    ]
}